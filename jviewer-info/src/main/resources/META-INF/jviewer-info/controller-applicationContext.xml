<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="newsController" class="tk.jviewer.info.controller.NewsController">
        <property name="manager" ref="newsService"/>
        <property name="newsModel" ref="newsModel"/>
        <property name="userModel" ref="userModel"/>
    </bean>

    <bean id="redirectController" class="tk.jviewer.info.controller.RedirectController"/>

    <bean id="loginController" class="tk.jviewer.info.controller.LoginController">
        <property name="securityService" ref="securityService"/>
        <property name="userModel" ref="userModel"/>
    </bean>

</beans>