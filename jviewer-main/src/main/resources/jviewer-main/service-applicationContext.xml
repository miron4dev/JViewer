<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="registrationService" class="tk.jviewer.service.impl.RegistrationServiceImpl">
        <property name="validationDao" ref="validationDao"/>
        <property name="registrationDao" ref="registrationDao"/>
        <property name="encoder" ref="encoder"/>
    </bean>

    <bean id="loginService" class="tk.jviewer.service.impl.LoginServiceImpl">
        <property name="loginDao" ref="loginDao"/>
        <property name="validationDao" ref="validationDao"/>
        <property name="encoder" ref="encoder"/>
    </bean>

    <bean id="managementController" class="tk.jviewer.controller.ManagementController">
        <property name="managementDao" ref="managementDao"/>
    </bean>

    <bean id="resourceService" class="tk.jviewer.service.impl.ResourceServiceImpl">
        <property name="dao" ref="resourceDao"/>
    </bean>

    <bean id="quizService" class="tk.jviewer.service.impl.QuizServiceImpl">
        <property name="quizDao" ref="quizDao"/>
        <property name="questionDao" ref="questionDao"/>
        <property name="answerDao" ref="answerDao"/>
        <property name="quizResultDao" ref="quizResultDao"/>
    </bean>

</beans>