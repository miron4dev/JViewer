<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="registrationDao" class="tk.jviewer.dao.impl.RegistrationDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="loginDao" class="tk.jviewer.dao.impl.LoginDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="validationDao" class="tk.jviewer.dao.impl.ValidationDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="managementDao" class="tk.jviewer.dao.impl.ManagementDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="resourceDao" class="tk.jviewer.dao.impl.ResourceDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="quizDao" class="tk.jviewer.dao.quiz.impl.QuizDaoImpl"/>

    <bean id="questionDao" class="tk.jviewer.dao.quiz.impl.QuestionDaoImpl"/>

    <bean id="answerDao" class="tk.jviewer.dao.quiz.impl.AnswerDaoImpl">
        <property name="questionDao" ref="questionDao"/>
    </bean>

</beans>